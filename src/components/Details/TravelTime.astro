---
interface Props{
    timetravel : string
}
const {timetravel} = Astro.props
---
<section class="flex justify-start flex-col items-start gap-4 mt-5">
    <h2 class="text-2xl md:text-3xl font-semibold relative">
        Duración de la Aventura
        <span class=" block w-full bg-[#7f3812] h-1 pt-2"></span>
    </h2>
    <ul>
        <li class="flex gap-2 text-2xl font-semibold" >⏳{timetravel}</li>
    </ul>
</section>